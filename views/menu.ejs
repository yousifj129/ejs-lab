<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Menu</title>
    <link rel="stylesheet" href="style.css" />
  </head>
  <body>
    <%- include('./partials/nav') %>

    <h1>Full Menu</h1>
    <ul>
      <% menu.forEach(item => {%>
      <li>Item name: <%= item.name %><br>
      price: <%= item.price %><br>
    rating: <%= item.rating %><br>
      Details: <%= item.details %></li>

      <%   }); %>
    </ul>
  </body>
</html>
